{
    "version": 1,
    "project": "snapi",
    "project_url": "https://github.com/kdesoto-astro/snapi",
    "repo": "..",
    "branches": [
        "HEAD"
    ],
    "install_command": [
        "python -m pip install {wheel_file}",
        "python -m pip install {wheel_file}[dev]"
    ],
    "build_command": [
        "python -m build --wheel -o {build_cache_dir} {build_dir}"
    ],
    "dvcs": "git",
    "environment_type": "virtualenv",
    "show_commit_url": "https://github.com/kdesoto-astro/snapi/commit/",
    "pythons": [
        "3.11"
    ],
    "matrix": {
        "Cython": [],
        "build": [],
        "packaging": []
    },
    "benchmark_dir": ".",
    "env_dir": "env",
    "results_dir": "_results",
    "html_dir": "_html",
    "build_cache_size": 8,
    "regressions_first_commits": {}
}